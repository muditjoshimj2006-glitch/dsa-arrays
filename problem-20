 //problem 20 missing number
#include <iostream>
using namespace std;

int main() {
    int arr[5] = {1,2,4,5};
    int n = sizeof(arr)/sizeof(arr[0]);
    int sum = 0;
    
    int result = n*(n+1)/2;
    
    for(int i = 0; i < n; i++){
        sum += arr[i];
    }
    
    int missing = result - sum;
    cout << "MISSING VALUE IS : " << missing << endl;
  
    return 0;
}
