#include <iostream>
using namespace std;

int main() {

    int A[5] = {1,2,3,4,5};
    int B[4] = {2,3,1,5};
    
    int n1 = sizeof(A)/sizeof(A[0]);
    int n2 = sizeof(B)/sizeof(B[0]);

    for(int i = 0; i < n1; i++){
        bool found = false;

        for(int j = 0; j < n2; j++){
            if(A[i] == B[j]){
                found = true;
                break;
            }
        }

        if(!found){
            cout << "Missing element : " << A[i];
        }
    }
}