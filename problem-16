 // two sum of 2 elements using brute force approach 
#include <iostream>
#include<climits>
using namespace std;

int main() {
    int arr[6] = {1,2,3,4,5,6};
    int n = sizeof(arr)/sizeof(arr[0]);
    int target = 4;
    
    for(int i = 0; i < n; i++){
        for(int j = i+1; j < n; j++){
            if(arr[i] + arr[j] == target){
                cout << arr[i] << "," << arr[j] << endl;
            }
        }
    }
   
    return 0;
}
