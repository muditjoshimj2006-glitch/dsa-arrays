 // problem 23 plus one
#include <iostream>
using namespace std;
int main() {
    int arr[3] = {1,2,9};
    int n = sizeof(arr)/sizeof(arr[0]);
    
    for(int i = n-1; i > 0; i--){
        if(arr[i] < 9){
            arr[i]++;
            break;
        }
        else if(arr[i] == 9){
            arr[i] = 0;
        }
        
    }
    
    for(int i = 0; i < n; i++){
        cout << arr[i] << " " ;
    }

    return 0;
}
