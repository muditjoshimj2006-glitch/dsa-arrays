 // key pair [problem 15]
#include <iostream>
using namespace std;

int main() {
    int arr[5] = {1,2,3,4,5};
    int target = 7;
    int n = sizeof(arr)/sizeof(arr[0]);
    bool check = false;
    
    for(int i = 0; i < n; i++){
        for(int j = i+1; j < n; j++){
            if(arr[i] + arr[j] == target){
                check = true;
            }
        }
    }
    if(check){
        cout << "Pair Found" << endl;
    }
    
    else{
        cout << "Pair not found" << endl;
    }

    return 0;
}
