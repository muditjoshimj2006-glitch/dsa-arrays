 // problem 10 (greatest on right)
#include <iostream>
using namespace std;

int main() {
    int arr[6] = {1,1,2,3,2,5};
    int n = sizeof(arr)/sizeof(arr[0]);
  
    
    for(int i = 0; i < n; i++){
        int max = 0;
        for(int j = i+1; j < n; j++){
            if(arr[j] > max){
                max = arr[j];
        
            }
        }
           cout << "Greatest number = " << max << endl;
           return 0;
    }

    return 0;
}
