 //problem 19 sort an array of 0, 1 and 2
#include <iostream>
using namespace std;

int main() {
    int arr[5] = {2,1,0,0,1};
    int n = sizeof(arr)/sizeof(arr[0]);
    int start = 0;
    int mid = 0;
    int end = n-1;
    
    while(start <= end){
        if(arr[mid] == 0){
            swap(arr[start], arr[mid])
            start++;
        }
        
        else if(arr[start] == 1){
            mid++;
        }
        
        else if(arr[mid] == 2){
            swap(arr[mid], arr[end]);
            end--;
        }
    }
  for(int i = 0; i < n; i++){
      cout << arr[i] << " ";
  }
    return 0;
}
