 // first repeating element [problem 13]
#include <iostream>
using namespace std;

int main() {
    int arr[5] = {1,2,1,3,2};
    int n = sizeof(arr)/sizeof(arr[0]);
    
    for(int i = 0; i < n; i++){
        for(int j = i+1; j < n; j++){
            if(arr[i] == arr[j]){
                cout << "First repeating element : " << arr[j] << endl;
                return 0;
            }
        }
    }

    return 0;
}
