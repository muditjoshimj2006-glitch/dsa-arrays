 // majority element using brute force approach
#include <iostream>
#include<climits>
using namespace std;

int main() {
    int arr[6] = {1,2,1,1,1,2};
    int n = sizeof(arr)/sizeof(arr[0]);
    int majority = n/2;
   
    for(int i = 0;i < n; i++){
        int freq = 0;
        for(int j = 0; j < n; j++){
            if(arr[i] == arr[j]){
                freq++;
            }
        }
        if(freq > majority){
           cout << "Majority element : " << arr[i] << endl;
           return 0;
    }
    
    else{
        cout <<"No majority element" << endl;
        return 0;
    }
    
        }
    
    
  
    return 0;
}
