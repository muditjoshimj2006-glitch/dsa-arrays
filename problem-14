 // intersection of 2 arrays [problem 14]
#include <iostream>
using namespace std;

int main() {
    int arr1[5] = {1,2,3,4,5};
    int arr2[5] = {4,5,6,7,8};
    int n = sizeof(arr1)/sizeof(arr1[0]);

    for(int i = 0; i < n; i++){
        for(int j = 0; j < n; j++){
            if(arr1[i] == arr2[j]){
                cout << arr1[i] << " ";
            }
        }
    }


    return 0;
}
