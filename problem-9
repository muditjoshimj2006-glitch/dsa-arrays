 // problem 9 (find duplicate in an array)
#include <iostream>
using namespace std;

int main() {
    int arr[5] = {1,1,2,3,2};
    int n = sizeof(arr)/sizeof(arr[0]);
    bool check = false;
    int count = 0;
 
    for(int i = 0; i < n; i++){
        for(int j = i+1; j < n; j++){
            if(arr[i] == arr[j]){
                cout << "duplicate element = " << arr[i] << endl;
                check = true;
            }
        }
    
    }
    
   if(check != true){
       cout << "no duplicate element = " << endl;
       //return 0;
   }
   
    return 0;
}
