 // Cyclically rotate array by one element in an array [PROBLEM 6]
#include <iostream>
#include<climits>
using namespace std;

int main() {
    int arr[6] = {1,2,3,4,5,6};
    // new arr = {6,1,2,3,4,5}
    int n = sizeof(arr)/sizeof(arr[0]);
    int start = 0;
    int end = n-1;
    int temp = arr[n-1];
    
    for(int i = n-1; i > 0; i--){
        arr[i] = arr[i-1];
    }
    
    arr[0] = temp;


   for(int i = 0; i < n; i++){
       cout << arr[i] << " ";
   }
    return 0;
}
