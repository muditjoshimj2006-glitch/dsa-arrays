 // problem 11 ( facing the sun )
#include <iostream>
#include<string>
using namespace std;

int main(){
    int arr[6] = {7,4,8,2,9};
    int n = sizeof(arr)/sizeof(arr[0]);
    int max = 0;
    
    for(int i = 0; i < n; i++){
       if(arr[i] > max){
           cout << arr[i] << " ";
           max = arr[i];
       }
      
    }
    
    return 0;
}
